# Test environment
PORT=3001
NODE_ENV=test

# Database - in-memory for tests
DATABASE_URL="file::memory:?cache=shared"

# JWT
JWT_SECRET=test-secret-key
JWT_EXPIRES_IN=1h

# Elasticsearch - disabled for tests
ELASTICSEARCH_NODE=http://localhost:9200
